			<!-- Receive Starting PUs (this is used because PUs territory balance is not completed
			<step name="germansPUsEndTurn" delegate="endTurn" player="Germans" maxRunCount="1" display="Receive Starting PUs"/>
			<step name="russiansPUsEndTurn" delegate="endTurn" player="Russians" maxRunCount="1" display="Receive Starting PUs"/>
